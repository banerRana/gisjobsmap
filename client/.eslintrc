{
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jasmine": true
    },
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "plugins": ["prettier", "react"],
    "parserOptions": {
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "prettier/prettier": "error",
        "react/prop-types": "off"
    }
}
